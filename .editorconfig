# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[**.rb]
indent_size = 2
indent_style = space
max_line_length = 120

[**.yml]
indent_size = 2
indent_style = space
